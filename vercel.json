{
  "version": 2,
  "builds": [
    {
      "src": "qms-frontend/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "qms-backend/api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "qms-backend/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "qms-frontend/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
